

    <div class="sign-up">
        <form method="get" asp-action="SignUp" asp-controller="Account">
            <div class="frame-wrapper">
                <div class="frame">
                    <div class="div-wrapper">
                        <div class="div">
                            <div class="frame-2">
                                <div class="frame-3"><img class="blogger" src="~/images/blogger.svg" /></div>
                                <div class="text">Create a new account</div>
                            </div>
                            <div class="frame-4">
                                <div class="frame-5">
                                    <div class="frame-5">
                                        <div class="button-default">
                                            <img class="img" src="~/images/google-1.svg" />
                                            <div>
                                                <a asp-action="WithAccount" asp-controller="Account">Sign Up with Google</a>
                                            </div>
                                        </div>
                                        <div class="frame-6">
                                            <div class="divider"><img class="vector" src="~/images/vector-2.svg" /></div>
                                            <div class="text-2">OR</div>
                                            <div class="vector-wrapper"><img class="vector" src="~/images/vector-1.svg" /></div>
                                        </div>
                                    </div>
                                    <div class="frame-7">
                                        <div class="frame-8">
                                            <div class="frame-9">
                                                <div class="text-3">First Name</div>
                                                <input class="button-default-2" placeholder="Enter your First Name" />
                                            </div>
                                            <div class="frame-9">
                                                <div class="text-3">Last Name</div>
                                                <input class="button-default-2" placeholder="Enter your Last Name" />
                                            </div>
                                            <div class="frame-9">
                                                <div class="text-3">Email Address</div>
                                                <input class="button-default-2" placeholder="Enter your email" />
                                            </div>
                                            <div class="frame-9">
                                                <div class="text-3">Squad Number</div>
                                                <select class="button-default-2" id="squadNumber" name="squadNumber">
                                                    <option value="" selected disabled>Select your Squad number</option>
                                                    <option value="1">Squad 12</option>
                                                    <option value="2">Squad 18</option>
                                                </select>
                                            </div>
                                            <div class="frame-9">
                                                <div class="text-3">Stack</div>
                                                <select class="button-default-2" id="stack" name="stack">
                                                    <option value="" selected disabled>Select your Stack</option>
                                                    <option value="1">Java</option>
                                                    <option value="2">Dot Net</option>
                                                    <option value="3">Node.Js</option>
                                                </select>
                                            </div>
                                            <div class="frame-9">
                                                <div class="text-3">Password</div>
                                                <input class="button-default-2" placeholder="****************" />
                                            </div>
                                            <div class="frame-9">
                                                <div class="text-3">Confirm Password</div>
                                                <input class="button-default-2" placeholder="****************" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="frame-12">
                                    <button class="CTA-default">
                                        <div class="text-6">Register</div>
                                    </button>
                                    <p class="p">
                                        <span class="span">Already have an account ? </span>
                                        <a asp-action="Login" asp-controller="Account">Sign In Here</a>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>

@model IEnumerable<DevsTutorialCenterMVC.Models.Api.BlogPostVM>

<partial name="_LandingPageNavPartial"/>

<div class="container mt-2">

    <div class="flex flex-col justify-start items-start mt-10 md:flex-row md:space-x-10 ">
        <div class="flex flex-col space-y-8">
            @foreach (var post in Model)
            {
                <partial name="Components/_BlogPostEditorItemPartial" model="@post"/>
            }
        </div>
    </div>
</div>